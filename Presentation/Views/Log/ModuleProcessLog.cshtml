@{
    ViewBag.Title = "Horizontal Layout";
    ViewBag.pTitle = "Horizontal Layout";
    ViewBag.pageTitle = "Horizontal";
    Layout = "~/Views/_Shared/Layouts/_layout_horizontal.cshtml";
}


<div class="row">
    <div class="col-12">
        <div class="row">
            <h4>Modül İşlem Kayıtları</h4>
            <hr>
            <div class="col-lg-6">
                <div class="mb-3">
                    <div class="form-check form-switch mb-3" dir="ltr">
                        <input class="form-check-input" type="checkbox" id="SwitchCheckSizesm" checked>
                        <label class="form-check-label" for="SwitchCheckSizesm">Modüllerin işlem kayıtları tutabilmesi için bu özelliği etkinleştirmelisiniz.</label>
                    </div>
                </div>
            </div>
            
            <hr>
        </div>

        <div class="tab-content" id="myTabContent" style="background-color:white;">
            <div class="tab-pane fade show active" id="home" role="tabpanel" aria-labelledby="home-tab">
                <div class="row">
                    <div class="card-body">
                        <div class="row">
                            <div class="col-lg-2">
                                <div class="mb-3">
                                </div>
                            </div>
                            <div class="col-lg-2">
                                <div class="mb-3">
                                </div>
                            </div>
                            <div class="col-lg-3">
                                <div class="mb-3">
                                </div>
                            </div>
                            <div class="col-lg-2">
                            </div>
                            <div class="col-lg-2">
                                <div class="mb-3">
                                    <input type="text" class="form-control" placeholder="Ara"></input>
                                </div>
                            </div>
                        </div>
                        <table class="table table-bordered dt-responsive nowrap w-100">
                            <thead>
                                <tr>
                                    <th>Türü</th>
                                    <th>Modül Adı</th>
                                    <th>Eylem Adı</th>
                                    <th>Tarih</th>
                                    <th>IP Adresi</th>
                                </tr>
                            </thead>

                            <tbody>
                                <tr>
                                    <td>Tiger Nixon</td>
                                    <td>System Architect</td>
                                    <td>Edinburgh</td>
                                    <td>61</td>
                                    <td>61</td>

                                </tr>
                            </tbody>
                        </table>
                    </div>
                </div>

            </div>

        </div> <!-- end col -->
    </div>
</div>

